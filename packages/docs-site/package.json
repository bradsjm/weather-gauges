{
  "name": "@bradsjm/weather-gauges-docs",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "description": "Documentation and playground site for Weather Gauges",
  "scripts": {
    "predev": "pnpm --filter @bradsjm/weather-gauges-core build && pnpm --filter @bradsjm/weather-gauges-elements build",
    "build": "vite build",
    "prebuild": "pnpm --filter @bradsjm/weather-gauges-core build && pnpm --filter @bradsjm/weather-gauges-elements build",
    "dev": "vite",
    "lint": "eslint \"src/**/*.ts\"",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "vitest run --passWithNoTests",
    "test:visual": "vitest run --passWithNoTests",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@bradsjm/weather-gauges-elements": "workspace:*"
  }
}
