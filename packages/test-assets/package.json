{
  "name": "@bradsjm/steelseries-v3-test-assets",
  "version": "0.0.0",
  "type": "module",
  "description": "Fixtures and visual baseline assets for SteelSeries v3",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "tsc -p tsconfig.json --watch",
    "lint": "eslint \"src/**/*.ts\"",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "vitest run src --passWithNoTests",
    "test:visual": "rm -rf ../../test-results/radial-bargraph-* && pnpm --filter @bradsjm/steelseries-v3-elements build && playwright test --config ../../playwright.config.ts",
    "test:visual:update": "rm -rf ../../test-results/radial-bargraph-* && pnpm --filter @bradsjm/steelseries-v3-elements build && playwright test --config ../../playwright.config.ts --update-snapshots",
    "clean": "rm -rf dist"
  }
}
